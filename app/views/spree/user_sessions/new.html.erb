<% @body_id = 'login' %>
<div id="login-title">
  <h1><%= Spree.t(:login_as_existing) %></h1>
  <hr>    
</div>
<div id="existing-customer" data-hook="login">
  <% if flash[:alert] %>
    <div class="alert alert-danger"><%= flash[:alert] %></div>
  <% end %>
  <%= render :partial => 'spree/shared/login' %>
  <div data-hook="login_extras"></div>
</div>